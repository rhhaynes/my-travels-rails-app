<div class="link-secondary-subcontainer">
  <p><%= link_to("Preview", travel_path(travel), :class => "travel-logs-index") %></p>
  <% if user == current_user %>
    <p><%= link_to("Delete", travel_path(travel), :method => :delete, :data => {:confirm => "Are you sure?"}) %></p>
    <p><%= link_to("Edit", edit_travel_path(travel)) %></p>
  <% end %>
</div>
